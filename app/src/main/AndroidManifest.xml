<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.is4401.sociallysafe">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".User.Cust_Search"></activity>
        <activity android:name=".Admin.Edit_Queue" />
        <activity android:name=".Login.SegregationPostRegistration" />
        <activity android:name=".User.User_CreateProfile" />
        <activity android:name=".User.User_MyQueues" />
        <activity android:name=".User.MainActivity" />
        <activity android:name=".User.User_Profile" />
        <activity android:name=".Admin.Admin_QueueConfigurated" />
        <activity android:name=".Login.SegregationActivity" />
        <activity android:name=".Login.FirebaseRegister" />

        <activity android:name=".Login.FirebaseLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Admin.Admin_OperateQueue" />
        <activity android:name=".Admin.Admin_Main" />
        <activity android:name=".User.User_QueueGallery" />
        <activity
            android:name=".Admin.VerifyLogin"
            android:label="Verify Account"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Admin.Admin_QueueActivity"
            android:label="Queue ACtivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Admin.Admin_AddCust"
            android:label="Admin Add Customer"
            android:theme="@style/AppTheme" />
    </application>

</manifest>